<script>
  document.addEventListener("DOMContentLoaded", () => {
    const tabButtons = document.querySelectorAll('.gift-page-product-type_type-tb-btn-');
    const products = document.querySelectorAll('.gift-page-product-type-prdct');

    tabButtons.forEach(button => {
      button.addEventListener('click', () => {
        const priceRange = button.getAttribute('data-price-range');

        // Hide all products
        products.forEach(product => {
          product.style.display = 'none';
        });

        // Show products that match the selected price range
        products.forEach(product => {
          if (product.getAttribute('data-price-range') === priceRange) {
            product.style.display = 'block';
          }
        });

        // Remove the active class from all tabs
        tabButtons.forEach(btn => {
          btn.classList.remove('gift-page-product-type_type-tb-btn--active');
        });

        // Add the active class to the clicked tab
        button.classList.add('gift-page-product-type_type-tb-btn--active');
      });
    });

    // Set the default tab and add the active class to it
    const defaultTab = document.querySelector('[data-price-range="For-him"]');
    if (defaultTab) {
      defaultTab.click(); // Simulate click to show default products
      defaultTab.classList.add('gift-page-product-type_type-tb-btn--active'); // Add active class
    }
  });
</script>
