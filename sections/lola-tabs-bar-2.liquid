 .gift-page-product-type-section {
    padding: 50px 20px;
  }
  
  .gift-page-product-type-tb {
    display: flex;
    margin-bottom: 15px;
    justify-content: center;
  }
  
  .gift-page-product-type_type-tb-btn- {
