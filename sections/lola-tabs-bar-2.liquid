    color: #575757;
    font-size: 15px;
    border-bottom: 1px solid #f4f4f4;
  }
  
  .gift-page-product-type_type-tb-btn--active {
    border-bottom: 1px solid;
    color: #212121;
  }
  
  .gift-page-product-type-prdct-contnr {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 20px;
  }
  
  .gift-page-product-type-prdct {
    display: none;
    text-align: center;
  }
  
  .gift-page-product-type-prdct img {
    max-width: 100%;
    height: 100%;
    width: 100%;
    aspect-ratio: 2 / 2;
    object-fit: cover;
    border: 1px solid #eeeeee;
  }
  
  .gift-page-product-type-prdct-image {
    height: auto;
  }
  
  .gift-page-product-type-prdct-details {
    margin-top: 10px;
    text-align: left;
  }
  
  .gift-page-product-type-prdct-details p {
    margin: 0px;
    color:#333;
  }

  @media only screen and (max-width: 768px) {
    .gift-page-product-type-section {
      padding: 30px 20px 20px;
    }
    
    .gift-page-product-type-prdct-contnr {
      grid-template-columns: repeat(2, 1fr);
    }

    .gift-page-product-type_type-tb-btn- {
      padding: 5px 15px;
      font-size: 12px;
      line-height: 17px;
