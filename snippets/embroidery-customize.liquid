  color: #2c272d;  
}
.Summar-icon-emb-wrap, .emb-variants-continue-btn{
  border: 1px solid #2c272d;
  padding: 16px 24px;
  width: 50%;
  font-size: 16px;
  font-weight: 600;
}
.emb-variants-continue-btn{
    border-radius: 0 100px 100px 0;  
    background-color: #2c272d;
    color:white; 
}
/* no space when closed */
.emb-words-letter-font-wrapper,
.emb-monogram-font-wrapper{
  overflow: hidden;
  max-height: fit-content;
  display: none;
  margin-top: 0;       /* <- ensure no margin while closed */
  padding: 0;          /* <- protect against padding creating height */
  border: 0;           /* <- if you had borders, they add height too */
  transition: max-height 260ms ease, opacity 260ms ease;
  will-change: max-height, opacity;
}

/* add spacing only when open */
.emb-words-letter-font-wrapper.is-open,
.emb-monogram-font-wrapper.is-open{
 display:block;
  margin-top: 24px;
}

