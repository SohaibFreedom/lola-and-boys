.page-banner-section {
    background: #ebebeb;
    display: inline-block;
    width: 100%;
    vertical-align: top;

    .page-banner-content {
        text-align: center;
        height: 260px;
        display: table;
        margin: 0 auto;

        @media (max-width: 1024px){
        	height: 200px;
        }

        .page-banner-content-inner {
            vertical-align: middle;
            display: table-cell;

            h1 {
                font-size: 35px;
                font-weight: 600;
                padding: 0;

                @media (max-width: 1024px){
                	font-size: 30px;
                }
                @media (max-width: 991px){
                	font-size: 25px;
                }
            }
            
            p {
                text-transform: uppercase;
                letter-spacing: 2px;
                font-family: 'Roboto';
