}


.trans-title {
    display: inline-block;
    width: 100%;
    vertical-align: top;
    padding: 50px 0;
    @media (max-width: 767px){
    	padding: 20px 0;
    }
}
.trans-title-main {
    text-align: center;

    .trans-title-inner {
        position: relative;

        h2 {
            font-weight: 700;
            color: #F5F5F5;
            font-size: 90px;
            margin: 0;
            letter-spacing: 10px;
            @media (max-width: 1024px){
            	font-size: 70px;
            }
            @media (max-width: 767px){
            	font-size: 37px;
            }
        }

        h5 {
            position: absolute;
            left: 0;
            top: 50%;
            right: 0;
            margin: 0 auto;
            transform: translate(0, -50%);
            font-size: 20px;
            padding: 0;
            font-weight: 500;

            @media (max-width: 1024px){
            	font-size: 17px;
            }
            @media (max-width: 991px){
            	font-size: 15px;
            }
        }
