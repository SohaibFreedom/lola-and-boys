.about-form-section{
	padding: 30px 0;

	.about-form-box {
	    text-align: center;
	    position: relative;
	    z-index: 2;
	    
	    ::after{
	    	content: "";
		    position: absolute;
		    left: -10px;
		    top: 10px;
		    width: 100%;
		    height: 100%;
		    border: 2px solid #000;
		    z-index: -9;
	    }

	    .about-form-box-inner {
	        border: 4px solid #000;
	        padding: 40px;
	        background: #fff;

	        h3 {
	            font-weight: 500;
	        }

	        input[type="text"] {
	            max-width: 740px;
	            margin: 0 auto;
	        }
	    }
	}
}

.application-section-img{

	img{
		width: 100%;
	}
}
.application-section-main {
    padding: 70px 0;

    @media (max-width: 1024px){
    	padding: 50px 0;
    }
    @media (max-width: 991px){
    	padding: 40px 0;
    }

    .application-section-content {
        text-align: center;
        margin-bottom: 80px;

        @media (max-width: 991px){
        	margin-bottom: 40px;
        }

        .application-content-text {
            max-width: 415px;
            margin: 0 auto;

            h3 {
                font-weight: 500;
                font-size: 20px;
            }

            ul {
                padding: 0;
                display: -webkit-box;
                display: -ms-flexbox;
                display: flex;
                -webkit-box-pack: justify !important;
                -ms-flex-pack: justify !important;
                justify-content: space-between !important;

                li {
                    margin: 0;
                    padding: 0 5px;

                    a {
                        display: inline-block;
                        vertical-align: top;

                        img {
                            vertical-align: top;

                            @media (max-width: 575px){
                            	max-width: 85px;
                            }
                        }
                    }
                }
            }
            .social-icon-listing{
            	ul{
            		li{
            			a {
            			    width: 32px;
        			        height: 32px;
        			        color: #fff;
        			        background: #000;
        			        border-radius: 50px;
        			        line-height: 32px;
        			        text-align: center;
        			        border: 1px solid #000;
        			        transition: all 0.5s ease 0s;

        			        &:hover{
        			        	background: transparent;
    			        	    color: #000;
        			        }
            			}
            		}
            	}
            }
        }

        &:last-child{
        	margin-bottom: 0;
        }
    }
}
.growing-box-content{
