.about-page-listing{
	padding: 50px 0;

	.trans-title-main{
		margin-bottom: 40px;
	}

	.about-listing{
		text-align: center;
		margin-bottom: 50px;
		h4 {
		    font-size: 18px;
		    font-weight: 500;
		    margin: 0;
		}
		> a {
		    font-size: 14px;
		    color: #000;
		}
		ul {
		    padding: 0;
	        -webkit-column-count: 4;
	        -moz-column-count: 4;
	        column-count: 4;
	        text-align: left;
	        width: calc(100% - 250px);
	        margin: 30px auto 0;

	        @media (max-width: 1024px){
	        	width: calc(100% - 50px);
	        }
	        @media (max-width: 991px){
	        	-webkit-column-count: 3;
	        	-moz-column-count: 3;
	        	column-count: 3;
	        }
	        @media (max-width: 767px){
	        	-webkit-column-count: 2;
	        	-moz-column-count: 2;
	        	column-count: 2;
	        	width: 100%;
	        }
	        @media (max-width: 575px){
	        	-webkit-column-count: 1;
	        	-moz-column-count: 1;
	        	column-count: 1;
	        }
		    li{
		    	a {
		    	    color: #000;
		    	    font-size: 14px;
				    font-family: 'Karla', sans-serif !important;
			        @media (max-width: 767px){
			        	text-decoration: underline;
			        }
		    	}
		    }
		}
	}
}


