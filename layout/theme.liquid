jQuery(function() {
  var Accordion = function(el, multiple) {
    this.el = el || {};
    this.multiple = multiple || false;

    var links = this.el.find('.accordion__title');
    links.on('click', {
      el: this.el,
      multiple: this.multiple
    }, this.dropdown)
  }

  Accordion.prototype.dropdown = function(e) {
    var $el = e.data.el;
    $this = jQuery(this),
      $next = $this.next();

    $next.slideToggle();
    $this.parent().toggleClass('accordion--open');

    if (!e.data.multiple) {
      $el.find('.accordion__content').not($next).slideUp().parent().removeClass('accordion--open');
    };
  }
  var accordion = new Accordion(jQuery('.accordion-container'), false);
});


      
    </script>
